{
  "presets": [
    ["next/babel",
      {
        "styled-jsx": {
          "plugins": [
            "styled-jsx-plugin-postcss"
          ]
        }
      },
      "@zeit/next-typescript/babel",
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }],
    ["@babel/plugin-proposal-class-properties", {
      "loose": true
    }],
    ["import", {
      "libraryName": "antd-mobile"
    }]
  ]
}